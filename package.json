{
  "name": "portsyncro",
  "version": "0.1.0",
  "private": true,
  "description": "Portfolio management app with demo account support. Set NEXT_PUBLIC_DEMO_EMAIL and NEXT_PUBLIC_DEMO_PASSWORD for demo login feature.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-env": "node scripts/check-env.js",
    "build:prod": "npm run check-env && next build",
    "start:prod": "npm run check-env && next start",
    "security:audit": "node scripts/security-audit.js",
    "security:check": "node scripts/security-check.js",
    "security:vulnerability": "node scripts/vulnerability-check.js",
    "security:fix": "npm audit fix && node scripts/replace-console-logs.js",
    "prebuild": "npm run security:audit",
    "predeploy": "npm run security:audit"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "firebase": "^12.7.0",
    "helmet": "^8.1.0",
    "helmet-csp": "^3.1.0",
    "next": "^16.1.1",
    "node-fetch": "^2.7.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8.5.6"
  }
}
